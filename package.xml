<package>
  <name>ati_mini_45</name>
  <version>0.5.0</version>
  <description>Driver for ATI Mini 45 FTS. Based on cob_forcetorque package.</description>
  <maintainer email="denis.stogl@kit.edu">Denis Å togl</maintainer>

  <license>LGPL</license>

  <url type="website">http://ros.org/wiki/ati_mini_45</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Alexander Bubeck</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>roscpp</build_depend>
  <build_depend>eigen</build_depend>
  <build_depend>tf2</build_depend>
	<build_depend>tf2_ros</build_depend>
	<build_depend>tf2_geometry_msgs</build_depend>
  <build_depend>cob_srvs</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>visualization_msgs</build_depend>
	<build_depend>geometry_msgs</build_depend>
  <build_depend>cob_generic_can</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>roscpp</run_depend>
  <run_depend>eigen</run_depend>
  <run_depend>tf2</run_depend>
	<run_depend>tf2_ros</run_depend>
	<run_depend>tf2_geometry_msgs</run_depend>
  <run_depend>cob_srvs</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>visualization_msgs</run_depend>
	<run_depend>geometry_msgs</run_depend>
  <run_depend>cob_generic_can</run_depend>

</package>
