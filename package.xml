<package format="2">
  <name>ati_mini_45</name>
  <version>0.9.0</version>
  <description>Driver for ATI Mini 45 FTS. Based on cob_forcetorque package.</description>
  <maintainer email="denis.stogl@kit.edu">Denis Å togl</maintainer>

  <license>LGPL</license>

  <url type="website">http://ros.org/wiki/ati_mini_45</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Alexander Bubeck</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <depend>roscpp</depend>
  <build_depend>message_generation</build_depend>
  <build_depend>cmake_modules</build_depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>std_msgs</depend>
  <depend>std_srvs</depend>
  <depend>visualization_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>cob_generic_can</depend>

  <!-- Dependencies needed after this package is compiled. -->
  <exec_depend>message_runtime</exec_depend>

</package>
