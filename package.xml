<package>
  <name>cob_forcetorque</name>
  <version>0.5.0</version>
  <description>Driver for the Schunk LWA 3 Force Torque Controller based on ATI Mini 45.</description>
  <maintainer email="denis.stogl@kit.edu">Denis Å togl</maintainer>

  <license>LGPL</license>

  <url type="website">http://ros.org/wiki/cob_forcetorque</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Alexander Bubeck</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>roscpp</build_depend>
  <build_depend>eigen</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>cob_srvs</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>libntcan</build_depend>
  <build_depend>visualization_msgs</build_depend>
  <build_depend>cob_generic_can</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>roscpp</run_depend>
  <run_depend>eigen</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>cob_srvs</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>libntcan</run_depend>
  <run_depend>visualization_msgs</run_depend>
  <run_depend>cob_generic_can</run_depend>

  <!-- Dependencies needed only for running tests. -->
